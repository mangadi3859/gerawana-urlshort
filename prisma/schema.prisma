generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    //DATABASE_URL="mysql://root:@localhost:3306/gerawana-shortener"
    url      = env("DATABASE_URL")
}

model User {
    id       Int     @id @default(autoincrement())
    nama     String
    email    String
    password String
    token    String?
}
